<odoo>
<!--
    Esta secuencia se utiliza para generar automáticamente
    un número único para cada paciente registrado.
  -->
  <data noupdate="1">
    <record id="seq_hospital_paciente" model="ir.sequence">
      <field name="name">Paciente</field>
      <field name="code">hospital.paciente</field>
      <field name="prefix">PA</field>
      <field name="padding">6</field>
      <field name="number_next">1</field>
      <field name="number_increment">1</field>
    </record>
  </data>
</odoo>
